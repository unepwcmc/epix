<%= render partial: "permits/mobile/permit_row_mobile",
  locals: {
    field_title: "5. #{t('special_conditions')}",
    values: [
      {
        value: @permit.special_conditions
      }
    ]
  }
%>
<%= render partial: "permits/mobile/permit_row_mobile",
  locals: {
    field_title: "5a. #{t('purpose_of_transaction')}",
    values: [
      {
        value: @permit.purpose_code
      },
      {
        value: @permit.purpose
      }
    ]
  }
%>
<%= render partial: "permits/mobile/permit_row_mobile",
  locals: {
    field_title: "5b. #{t('security_stamp_no')}",
    values: [
      {
        value: @permit.security_stamp_no
      }
    ]
  }
%>
<%= render partial: "permits/mobile/permit_row_mobile",
  locals: {
    field_title: "6. #{t('management_authority')}",
    values: [
      {
        field: t('name'),
        value: @permit.issuing_authority_name
      },
      {
        field: t('address'),
        value: @permit.issuing_authority_postal_address
      },
      {
        field: t('country'),
        value:
          """
            #{@permit.issuing_authority_country}
            #{@permit.issuing_authority_representative_person}
          """
      }
    ]
  }
%>
